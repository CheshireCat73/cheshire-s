#include <locale.h>
#include <iostream>//Подключение библиотек
#include <stdio.h>
int main(){
    int s,n;
    setlocale(LC_ALL, "Russian");
    printf("Введите число N\n");//Ввод значений пользователем 
    scanf("%d",&n);
    s=n%60;//Берем остаток от деления на 60
    printf("С начала последней минуты прошло секунд:\n");//Вывод значения
    printf("%d\n",s);}
