#include <math.h>//Подключение библиотек
#include <stdio.h>
int a,b,c;//Объявление переменных
double x,y,z,w;
int main(){//Начальная точка программы
    printf("Введите координаты для a и b 'x1,y1,x2,y2'\n");//Ввод координат пользователем 
    scanf("%lf%lf%lf%lf",&x,&y,&z,&w);//Считывание значений переменных
    c=(((z-x)*(z-x))+((w-y)*(w-y)));//Вычисление значения переменной
    printf("sqrtf : %.4f\n", sqrtf (c));//Вывод квадратного корня 
    return 0;
}
