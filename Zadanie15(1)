#include <iostream>

using namespace std;

double PowerA3(double A, double& B) {
    B = A*A*A;
    return B;
}

int main()
{
    double n1, n2;
    for(int i = 0; i < 5; i++) {
        cout<<"Введите число " << i + 1 << endl;
        cin>>n1;
        cout<<"Ответ: " << PowerA3(n1, n2) << endl;
    }
    
    return 0;
}
