    std::string c,d;
    setlocale(LC_ALL, "Russian");
    cout << "Число:" << endl;//Ввод значений пользователем
    cin >> i;
    a= i / 10;
    b=i % 10;
    const char* p[9];
    p[0] = "одно учебное задание";
    p[1] = "два учебных задания";
    p[2] = "три учебных задания";
    p[3] = "четыре учебных задания";
    p[4] = "пять учебных заданий";
    p[5] = "шесть учебных заданий";
    p[6] = "семь учебных заданий";
    p[7] = "восемь учебных заданий";
    p[8] = "девять учебных заданий";
    const char*m[3];
    m[0]="двадцать ";
    m[1]="тридцать ";
    m[2]="сорок ";
    if (a==1){
        if (b==0) {
            cout << "десять учебных заданий";
        }
        if (b==1){
            cout <<"одиннадцать учебных заданий";
        }
        if (b==2){
            cout <<"двенадцать учебных заданий";
        }
        if (b==3){
            cout <<"тринадцать учебных заданий";
        }
        if (b==4){
            cout <<"четырнадцать учебных заданий";
        }
        if (b==5){
            cout <<"пятнадцать учебных заданий";
        }
        if (b==6){
            cout <<"шестнадцать учебных заданий";
        }
        if (b==7){
            cout <<"семнадцать учебных заданий";
        }
        if (b==8){
            cout <<"восемнадцать учебных заданий";
        }
        if (b==9){
            cout <<"девятнадцать учебных заданий";
        }
    }
    else {
        if(a==2){
            c=m[0];}
        if(a==3){
            c=m[1];}
        if(a==4){
            c=m[2];}
        if(b==1){
            d=p[0];}
        if(b==2){
            d=p[1];}
        if(b==3){
            d=p[2];}
        if(b==4){
            d=p[3];}
        if(b==5){
            d=p[4];}
        if(b==6){
            d=p[5];}
        if(b==7){
            d=p[6];}
        if(b==8){
            d=p[7];}
        if(b==9){
            d=p[8];}
        if(b==0){
            cout << c;
        }
        else {
            cout << c << d;
        }
    }
}
