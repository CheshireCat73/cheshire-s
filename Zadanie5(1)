#include <math.h>//Подключение библиотек
#include <stdio.h>
int a,b,c;//Объявление переменных
double x,y,z,w;
int main(){//Начальная точка программы
    printf("Введите координаты для a и b 'x,y,z,w'\n");//Ввод координат пользователем 
    scanf("%lf%lf%lf%lf",&x,&y,&z,&w);//Считывание значений переменных
    c=(((y-x)*(y-x))+((w-z)*(w-z)));//Вычисление значения переменной
    printf("sqrtf : %.20f\n", sqrtf (c));//Вывод квадратного корня 
    return 0;
}
