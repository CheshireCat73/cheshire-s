#include <stdio.h> //Подключение библиотек
#include <iostream>
#include <cstdlib>
using namespace std;
//int main(){
struct bin_node
{
int data;
struct bin_node *left;
struct bin_node *right;
};
struct bin_tree
{
struct bin_node *root; //Указатель на корень дерева
int count; //Количество вершин в дереве
};
struct bin_tree *bin_create(void);
struct bin_tree *tree=(struct bin_tree *) malloc (sizeof *tree);//*выделение памяти. malloc выделяет память и возвращает указатель на нее
if(tree==NULL) //если памяти недостаточно
return NULL;
tree->root=NULL; //Дерево пустое
tree->count=0;
int main(){
return bin_tree*tree;
}
